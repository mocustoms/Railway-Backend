{
  "name": "easymauzo-backend",
  "version": "1.0.0",
  "description": "EasyMauzo POS System - Backend API Server",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "migrate": "node scripts/run-migrations.js",
    "migrate:verify": "node scripts/run-migrations.js && node scripts/verify-database-schema.js",
    "verify-schema": "node scripts/verify-database-schema.js",
    "verify-schema:verbose": "node scripts/verify-database-schema.js --verbose",
    "sync-migrations": "node scripts/sync-migrations-to-current-state.js",
    "reset-migrations": "node scripts/reset-migrations.js",
    "reset-db": "node scripts/reset-database-and-migrate.js",
    "reset-admin": "node scripts/resetAdminPassword.js",
    "seed-admin": "node scripts/seedAdminUser.js",
    "backup": "node scripts/backup-local-db-node.js",
    "backup:pg": "node scripts/backup-local-db.js",
    "backup:pgdump": "node scripts/backup-local-pgdump.js",
    "backup:helper": "node scripts/backup-and-restore-helper.js",
    "restore": "node scripts/restore-to-railway-node.js",
    "restore:pg": "node scripts/restore-to-railway.js",
    "migrate:railway": "node scripts/run-migrations-railway.js",
    "verify:railway": "node scripts/verify-railway-schema.js",
    "reset:migrations:railway": "node scripts/reset-railway-migrations.js",
    "railway": "node scripts/railway-db-manager.js",
    "railway:sync": "node scripts/railway-db-manager.js sync",
    "railway:verify": "node scripts/railway-db-manager.js verify",
    "railway:compare": "node scripts/railway-db-manager.js compare",
    "railway:migrate": "node scripts/railway-db-manager.js migrate",
    "railway:quick": "node scripts/railway-db-manager.js quick",
    "railway:sync-columns": "node scripts/sync-railway-columns-to-local.js",
    "clean:numeric": "node scripts/clean-malformed-numeric-values.js"
  },
  "engines": {
    "node": ">=20.16.0",
    "npm": ">=10.0.0"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.5.0",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.2.1",
    "form-data": "^4.0.4",
    "helmet": "^7.1.0",
    "html-pdf-node": "^1.0.8",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.1",
    "node-cron": "^4.2.1",
    "node-fetch": "^2.7.0",
    "password-validator": "^5.2.1",
    "pdfkit": "^0.17.1",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "puppeteer": "^24.10.2",
    "sequelize": "^6.35.1",
    "sharp": "^0.34.2",
    "sqlite3": "^5.1.7",
    "supertest": "^7.1.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "pos",
    "backend",
    "api",
    "nodejs",
    "postgresql",
    "express"
  ],
  "author": "EasyMauzo Team",
  "license": "MIT"
}
